{
  "manifest_version": 2,
  "key": "",

  "name": "Conic example",
  "description": "This extension demonstrates how chrome extension may communicate with native application ",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabs",
    "nativeMessaging",
    "background"
  ],

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": "img/icon.png"

  }


}